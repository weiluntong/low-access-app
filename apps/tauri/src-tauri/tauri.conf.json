{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "LOW Access",
  "identifier": "low.access.desktop",
  "build": {
    "beforeDevCommand": "cd /home/weilunt/sso/frontend/apps/expo && yarn web",
    "devUrl": "http://localhost:8081",
    "beforeBuildCommand": "cd /home/weilunt/sso/frontend/apps/expo && yarn build:web",
    "frontendDist": "../../expo/dist"
  },
  "app": {
    "windows": [
      {
        "title": "LOW Access",
        "width": 1000,
        "height": 700,
        "center": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true,
      "capabilities": ["default"]
    },
    "withGlobalTauri": true
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "deepLink": {
      "mobile": [],
      "desktop": {
        "schemes": ["low.access.desktop"]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
